<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="idkjava.thelements" android:versionCode="63" android:versionName="6.0"> 
    <application android:label="@string/app_name" android:icon="@drawable/icon2">
    	<activity android:name=".MenuActivity"
    			  android:screenOrientation="portrait"
    			  android:label="@string/app_name"
    			  android:configChanges="keyboard|keyboardHidden|orientation|screenLayout|fontScale">
    		<intent-filter>
    			<action android:name="android.intent.action.MAIN" /> 
    			<category android:name="android.intent.category.LAUNCHER" />
    		</intent-filter>
    	</activity>
    	

		<meta-data android:name="com.mobclix.APPLICATION_ID" android:value="08768ce7-859d-4f21-b2b7-f8eed8c6da74"/>
    	<activity android:name=".SplashActivity"
    			  android:label="@string/app_name"
    			  android:screenOrientation="portrait"
    			  android:theme="@android:style/Theme.NoTitleBar"
    			  android:configChanges="keyboard|keyboardHidden|orientation|screenLayout|fontScale">
    		<intent-filter>
    			<action android:name="android.intent.action.MAIN" />
    			<category android:name="android.intent.category.DEFAULT" />
    		</intent-filter>
    	</activity>
        <activity android:name=".MainActivity"
				  android:screenOrientation="portrait"
                  android:label="@string/app_name"
    			  android:configChanges="keyboard|keyboardHidden|orientation|screenLayout|fontScale">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
            <intent-filter>
            	<action android:name="android.intent.action.VIEW" />
            	<category android:name="android.intent.category.DEFAULT" />
            	<category android:name="android.intent.category.BROWSABLE" />
				<data android:scheme="file" android:host="*" android:pathPattern=".*\\.sav"/>
            </intent-filter>
        </activity>
        <activity android:name=".preferences.PreferencesFromCode"
                  android:label="@string/app_name"
    			  android:configChanges="keyboard|keyboardHidden|orientation|screenLayout|fontScale">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
         <activity android:name=".CustomElementActivity"
                  android:label="@string/app_name"
    			  android:configChanges="keyboard|keyboardHidden|orientation|screenLayout|fontScale">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
            <intent-filter>
            	<action android:name="android.intent.action.VIEW" />
            	<category android:name="android.intent.category.DEFAULT" />
            	<category android:name="android.intent.category.BROWSABLE" />
				<data android:scheme="file" android:host="*" android:pathPattern=".*\\.ele"/>
            </intent-filter>
        </activity>
        <activity android:name=".network.networklogin"
                  android:label="@string/app_name"
    			  android:configChanges="keyboard|keyboardHidden|orientation|screenLayout|fontScale">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".network.networkengage"
       			  android:screenOrientation="portrait"
                  android:label="@string/app_name"
                  android:theme="@android:style/Theme.NoTitleBar"
    			  android:configChanges="keyboard|keyboardHidden|orientation|screenLayout|fontScale">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>
    
    <uses-sdk android:minSdkVersion="4" android:targetSdkVersion="8"/>
    
	<uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
	<uses-permission android:name="android.permission.INTERNET" />
	<uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
</manifest> 
